{
  "name": "neighborhood-mapping-frontend",
  "version": "1.1.0",
  "description": "近隣情報マッピングアプリのフロントエンド (TypeScript版)",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "dev": "python3 -m http.server 3001 --directory public",
    "type-check": "tsc --noEmit",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "start": "../start_frontend.sh",
    "prebuild": "npm run clean",
    "postbuild": "echo 'TypeScript compiled successfully. Files available in dist/'",
    "serve": "npm run build && npm run dev"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/leaflet": "^1.9.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "mapping",
    "crime-data",
    "schools",
    "tokyo",
    "typescript",
    "react",
    "leaflet"
  ],
  "author": "Neighborhood Mapping Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "."
  },
  "bugs": {
    "url": "https://github.com/neighborhood-mapping/issues"
  },
  "homepage": "http://localhost:3001"
}