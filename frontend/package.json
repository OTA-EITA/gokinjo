{
  "name": "neighborhood-mapping-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 20",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist .lighthouseci",
    "lighthouse": "npm run clean && npm run build && npx lhci autorun",
    "lighthouse:debug": "npm run clean && npm run build && npx lhci autorun --verbose",
    "lighthouse:quick": "npm run clean && npm run build && LHCI_RUNS=1 npx lhci autorun"
  },
  "dependencies": {
    "chart.js": "^4.4.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "leaflet": "^1.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "http-server": "^14.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
